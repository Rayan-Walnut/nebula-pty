{
  "name": "nebula-pty",
  "version": "1.0.0",
  "description": "Custom PTY module for terminal emulation in Electron applications",
  "main": "index.js",
  "scripts": {
    "build": "node-gyp rebuild",
    "start": "electron .",
    "test": "node test/test-interactive.js",
    "check-deps": "scripts/check.bat",
    "rebuild": "electron-rebuild",
    "clean": "node-gyp clean",
    "prepare": "npm run build",
    "postinstall": "npm run rebuild"
  },
  "keywords": [
    "terminal",
    "pty",
    "node-addon",
    "electron",
    "xterm",
    "shell",
    "console"
  ],
  "author": "Rayan <prorayanelmanssouri@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rayan-Walnut/nebula-pty.git"
  },
  "bugs": {
    "url": "https://github.com/Rayan-Walnut/nebula-pty/issues"
  },
  "homepage": "https://github.com/Rayan-Walnut/nebula-pty#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@xterm/xterm": "^5.5.0",
    "express": "^4.21.2",
    "iconv-lite": "^0.6.3",
    "nan": "^2.22.0",
    "node-addon-api": "^8.3.0",
    "ws": "^8.18.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "node-gyp": "^10.0.1",
    "electron": "^28.0.0",
    "electron-rebuild": "^3.2.0",
    "cmake-js": "^7.2.1"
  },
  "binary": {
    "module_name": "terminal",
    "module_path": "./build/Release/",
    "host": "https://github.com/Rayan-Walnut/nebula-pty/releases/download/",
    "remote_path": "{version}"
  }
}