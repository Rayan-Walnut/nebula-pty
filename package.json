{
  "name": "nebula-pty",
  "version": "1.0.5",
  "description": "Custom PTY module for terminal emulation in Electron applications",
  "main": "index.js",
  "files": [
    "index.js",
    "binding.gyp",
    "src/**/*",
    "build/Release/terminal.node",
    "scripts/",
    "README.md",
    "CMakeLists.txt"
  ],
  "scripts": {
    "install": "npm config set python \"C:\\Users\\prora\\AppData\\Local\\Programs\\Python\\Python313\\python.exe\" && node-gyp rebuild",
    "build": "node-gyp rebuild",
    "start": "electron .",
    "test": "node test/test-interactive.js"
  },
  "binary": {
